объекты:
  человек;
;

инструменты:
  *проезжаяЧасть1: зона, цвет=rgb(255, 0, 0), толщина=2, состав=...(
    прямая, от=.(323,164), до=.(363,164);
    прямая, от=.(363,164), до=.(142,464);
    прямая, от=.(142,464), до=.(  0,464);
    прямая, от=.(  0,464), до=.(  0,333);
    прямая, от=.(  0,333), до=.(323,164)
  );

  *проезжаяЧасть2: зона, цвет=rgb(255, 0, 0), толщина=2, состав=...(
    прямая, от=.(443,164), до=.(473,164);
    прямая, от=.(473,164), до=.(848,333);
    прямая, от=.(848,333), до=.(848,464);
    прямая, от=.(848,464), до=.(758,464);
    прямая, от=.(758,464), до=.(443,164)
  );
;

обработка:
    если (человек.входитВ(*проезжаяЧасть1) или человек.входитВ(*проезжаяЧасть2) или
          человек.находитсяВ(*проезжаяЧасть1) или человек.находитсяВ(*проезжаяЧасть2)):
        высветить(человек, rgb(255, 255, 255));
    ;
;