объекты:
  автомобиль;
;

инструменты:
  *л: прямая, от=.(0, 363), до=.(848, 363), цвет=rgb(255, 122, 122);
;

обработка:
  если (автомобиль.пересекает(*л, сверху)):
    высветить(автомобиль, rgb(0, 255, 0));
  ;
  
  если (автомобиль.пересекает(*л, снизу)):
    высветить(автомобиль, rgb(255, 0, 0));  
  ;
;